<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
												   xmlns:tools="http://schemas.android.com/tools"
												   xmlns:app="http://schemas.android.com/apk/res-auto"
												   android:id="@+id/main" android:layout_width="match_parent"
												   android:layout_height="match_parent" tools:context=".MainActivity">
<Button
			android:text="Mostrar Datos"
			android:layout_width="wrap_content"
			android:layout_height="wrap_content" android:id="@+id/MostrarApikotlin"
			android:onClick="crearmostrarcategories"
			app:layout_constraintStart_toStartOf="parent" app:layout_constraintEnd_toEndOf="parent"
			app:layout_constraintBottom_toBottomOf="parent" app:layout_constraintHorizontal_bias="0.139"
			app:layout_constraintTop_toTopOf="parent" app:layout_constraintVertical_bias="0.566"/>
	<androidx.recyclerview.widget.RecyclerView
			android:id="@+id/RecyPersonas"
			android:layout_width="159dp"
			android:layout_height="166dp"
			app:layout_constraintTop_toTopOf="parent" app:layout_constraintBottom_toBottomOf="parent"
			app:layout_constraintStart_toStartOf="parent" app:layout_constraintEnd_toEndOf="parent"
			app:layout_constraintHorizontal_bias="0.15" app:layout_constraintVertical_bias="0.8"/>
	<Button
			android:text="Segunda Actividad"
			android:layout_width="wrap_content"
			android:layout_height="wrap_content" android:id="@+id/buttonSegundaActividad"
			app:layout_constraintStart_toStartOf="parent" app:layout_constraintEnd_toEndOf="parent"
			app:layout_constraintBottom_toBottomOf="parent" app:layout_constraintTop_toTopOf="parent"
			app:layout_constraintHorizontal_bias="0.938" app:layout_constraintVertical_bias="0.923"/>

</androidx.constraintlayout.widget.ConstraintLayout>